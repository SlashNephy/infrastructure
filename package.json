{
  "name": "k8s-manifests",
  "version": "1.0.0",
  "repository": "https://github.com/SlashNephy/k8s-manifests",
  "author": "SlashNephy <spica@starry.blue>",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check ."
  },
  "packageManager": "yarn@3.3.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.2.33",
    "@slashnephy/prettier-config": "0.1.1",
    "@types/eslint": "^8",
    "@types/jest": "29.2.5",
    "@types/node": "18.11.18",
    "@types/prettier": "^2",
    "concurrently": "7.6.0",
    "eslint": "8.31.0",
    "jest": "29.3.1",
    "prettier": "2.8.1",
    "ts-jest": "29.0.3",
    "typescript": "4.9.4"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
