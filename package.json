{
  "name": "k8s-manifests",
  "version": "1.0.0",
  "author": "SlashNephy <spica@starry.blue>",
  "repository": "https://github.com/SlashNephy/k8s-manifests",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "snapshot": "jest -u",
    "test": "jest"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.2.43",
    "@slashnephy/prettier-config": "0.1.9",
    "@types/eslint": "8.4.10",
    "@types/jest": "29.4.0",
    "@types/node": "18.11.18",
    "@types/prettier": "2.7.2",
    "concurrently": "7.6.0",
    "eslint": "8.33.0",
    "jest": "29.4.1",
    "prettier": "2.8.3",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "packageManager": "yarn@3.3.1",
  "engines": {
    "node": ">=18"
  },
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  },
  "jest": {
    "preset": "ts-jest"
  }
}
